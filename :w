)

type ChannelRepoImplMongo struct {
	Db *mongo.Database
}

func NewChannelRepo(db *mongo.Database) repository.ChannelRepo {
	return &ChannelRepoImplMongo{
		Db: db,
	}
}

func InsertOpenChannelRequest(context.Context, *models.OpenChannelRequest) error {
	return nil
}
